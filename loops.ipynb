    "# 2. Hollow Rectangle\n",
    "\n",
    "# *****\n",
    "# *   *\n",
    "# *****\n",
    "\n",
    "num = int(input(\"Enter the range: \"))\n",
    "for i in range(num):\n",
    "    for j in range(num):\n",
    "        if i == 0 or j == 0 or i == num - 1 or j == num - 1:\n",
    "            print('*', end = '')\n",
    "        else:\n",
    "            print(' ', end = '')\n",
    "    print(end = '\\n')"
   ]
